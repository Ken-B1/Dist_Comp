<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-ejb-jar PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 EJB 3.1//EN" "http://glassfish.org/dtds/glassfish-ejb-jar_3_1-1.dtd">
<glassfish-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>categoryBean</ejb-name>
      <jndi-name> java:global/statistics_EJB/categoryBean!services.categoryBeanInterface</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>searchBean</ejb-name>
      <jndi-name> java:global/statistics_EJB/searchBean!services.searchBeanInterface</jndi-name>
