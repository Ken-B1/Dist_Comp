    </ejb> 
    <ejb>
      <ejb-name>Registration</ejb-name>
      <jndi-name>java:global/statistics_EJB/Registration!services.RegistrationBeanInterface</jndi-name>
    </ejb> 
    <ejb>
      <ejb-name>LoginBean</ejb-name>
